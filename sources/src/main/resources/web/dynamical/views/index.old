<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<!DOCTYPE html>
<html xmlns:zk="http://www.zkoss.org/2005/zk" xmlns:z="http://www.zkoss.org/2005/zul">

<head>
<meta charset="UTF-8"></meta>
<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no'
	name='viewport'></meta>

<!-- Bootstrap 3.3.2 -->
<link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css" rel="stylesheet"
	type="text/css" />
<!-- Font Awesome Icons -->
<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css"
	rel="stylesheet" type="text/css" />
<!-- Ionicons -->
<link href="http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css" rel="stylesheet"
	type="text/css" />
<!-- Theme style -->
<link href="${contextPath}/zkau/web/dynamical/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />
<!-- AdminLTE Skins. Choose a skin from the css/skins 
         folder instead of downloading all of them to reduce the load. -->
<link href="${contextPath}/zkau/web/dynamical/css/skins/skin-blue.min.css" rel="stylesheet" type="text/css" />

<link href="${contextPath}/zkau/web/dynamical/css/dynamical.css" rel="stylesheet" type="text/css" />

<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
			<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
			<![endif]-->

<title>${applicationInfo.name} - ${navManager.currentPage.name}</title>
</head>

<body class="skin-blue fixed">
	<!-- header logo: style can be found in header.less -->
	<header class="header">
		<a href="${contextPath}" class="logo"> <img id="imglogo"
			src="${contextPath}${applicationInfo.defaultLogo}" class="img-responsive"
			title="${applicationInfo.name} - ${accountContext.account.name}" />
		</a>
		<!-- Header Navbar: style can be found in header.less -->
		<nav class="navbar navbar-static-top" role="navigation">
			<!-- Sidebar toggle button-->
			<a id="" href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button"> <span
				class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span>
				<span class="icon-bar"></span>
			</a>
			<div class="navbar-right">
				<ul class="nav navbar-nav">

					<!-- Messages: style can be found in dropdown.less-->
					<li class="dropdown messages-menu"><a href="#" class="dropdown-toggle"
						data-toggle="dropdown"> <i class="fa fa-envelope"></i> <span class="label label-success">0</span>
					</a>
						<ul class="dropdown-menu">
							<li class="header">No tienes mensajes</li>
							<li>
								<!-- inner menu: contains the actual data -->
								<ul class="menu">

								</ul>
							</li>
							<li class="footer"><a href="#">Ver todos</a></li>
						</ul></li>

					<!-- User Account: style can be found in dropdown.less -->
					<li class="dropdown user user-menu"><a href="#" class="dropdown-toggle"
						data-toggle="dropdown"> <i class="fa fa-user"></i> <span>
								${usuarioActual.nombre} <i class="caret"></i>
						</span>
					</a>
						<ul class="dropdown-menu">
							<!-- User image -->
							<li class="user-header bg-light-blue"><z:entityfileImage value="${usuarioActual.foto}"
									thumbnailHeight="90" thumbnailWidth="90" thumbnail="true" sclass="img-circle" />
								<p>
									${usuarioActual.nombreReal} <small>Miembro desde
										${usuarioActual.usuario.creationDate}</small>
								</p></li>
							<!-- Menu Body -->

							<!-- Menu Footer-->
							<li class="user-footer">
								<div class="pull-left">
									<z:a onClick="usuarios.showCambiarPassword()" class="btn btn-default btn-flat"
										label="Perfil" />
								</div>
								<div class="pull-right">
									<a href="${contextPath}/logout" class="btn btn-default btn-flat">Cerrar Sesion</a>
								</div>
							</li>
						</ul></li>
				</ul>
			</div>
		</nav>
	</header>
	<div id="sidebar" class="wrapper row-offcanvas row-offcanvas-left">
		<!-- Left side column. contains the logo and sidebar -->
		<aside class="left-side sidebar-offcanvas">
			<!-- sidebar: style can be found in sidebar.less -->
			<section class="sidebar">
				<!-- Sidebar user panel -->
				<div class="user-panel">
					<div class="pull-left image">
						<z:entityfileImage value="${usuarioActual.foto}" thumbnailHeight="90" thumbnailWidth="90"
							thumbnail="true" sclass="img-circle" />
					</div>
					<div class="pull-left info">
						<p>Hola, ${usuarioActual.nombre}</p>

						<a href="#"> <i class="fa fa-circle text-success"></i> En linea
						</a>
					</div>
				</div>
				<!-- search form -->
				<form action="#" method="get" class="sidebar-form">
					<div class="input-group">
						<input type="text" name="q" class="form-control" placeholder="Search..." /> <span
							class="input-group-btn">
							<button type='submit' name='seach' id='search-btn' class="btn btn-flat">
								<i class="fa fa-search"></i>
							</button>
						</span>
					</div>
				</form>
				<!-- /.search form -->
				<!-- sidebar menu: : style can be found in sidebar.less -->
				<z:div sclass="sidebar-builder" apply="${navBuilder}">
					<zk:custom-attributes builderClass="tools.dynamia.app.dynamical.DynamicalMenuBuilder" />
				</z:div>

			</section>
			<!-- /.sidebar -->
		</aside>

		<!-- Right side column. Contains the navbar and content of the page -->
		<aside class="right-side">
			<!-- Content Header (Page header) -->
			<section class="content-header">
				<h1 class="hidden-xs">
					<span id="pageTitle">${navManager.currentPage.name}</span> <small id="pageGroupTitle">${navManager.currentPageGroup.name}</small>
				</h1>

				<ol class="breadcrumb">
					<li><a href="/"> <i class="fa fa-dashboard"></i> Home
					</a></li>
					<li>${navManager.activeModule.name}</li>
					<li class="active"><z:a onClick="navManager.setCurrentPage(navManager.getLastPage())">${navManager.currentPage.name}</z:a>
					</li>
				</ol>
			</section>


			<!-- Main content -->
			<section class="content">
				<z:div id="workspace" zclass="row" apply="${navComposer}">
					<zk:custom-attributes builderClass="workspace.builders.Basic" />
				</z:div>
			</section>
		</aside>
		<!-- /.right-side -->
	</div>
	<!-- ./wrapper -->

	<!-- jQuery 2.1.3 -->
    <script src="${contextPath}/zkau/web/dynamical/js/jQuery-2.1.3.min.js"></script>
    <!-- Bootstrap 3.3.2 JS -->
    <script src="${contextPath}/zkau/web/dynamical/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- SlimScroll -->
    <script src="${contextPath}/zkau/web/dynamical/js/jquery.slimScroll.min.js" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='${contextPath}/zkau/web/dynamical/js/fastclick.min.js'></script>
    <!-- AdminLTE App -->
    <script src="${contextPath}/zkau/web/dynamical/js/app.min.js" type="text/javascript"></script>

	<script src="${contextPath}/zkau/web/dynamical/js/dynamical.js" type="text/javascript"></script>
</body>
</html>